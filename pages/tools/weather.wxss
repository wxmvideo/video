	/* 七天 */
	.sec-days {
		background: #FFFFFF;
		border-top: 2rpx solid #e4e4e4;
		border-bottom: 2rpx solid #e4e4e4;
		background-color: #fff;
		height: 630rpx;
		padding-top: 50rpx;
		margin-bottom: 20rpx;
		position: relative;
		overflow-x: auto;
		overflow-y: hidden;
		-webkit-overflow-scrolling: touch;
		display: block;
		max-width: 1000rpx;
		margin: 0 auto;
		box-sizing: border-box;
		margin-top: 20rpx;
	}

	.sec-days-scroll {
		width: 150%;
		overflow: hidden;
	}

	.sec-days-weather {
		box-sizing: border-box;
		width: 100%;
	}

	.sec-days .item {
		float: left;
		text-align: center;
		width: calc(100% / 8);
	}

	.sec-days .item-day,
	.sec-days .item-date {
		font-size: 24rpx;
		color: #999;
		line-height: 24rpx;
		height: 24rpx;
	}

	.sec-days .item-date {
		margin: 10rpx 0 30rpx;
	}

	.sec-days .item-daytime-weather {
		margin-bottom: 28rpx;
		font-size: 28rpx;
		color: #434343;
		line-height: 28rpx;
		height: 28rpx;
		display: block;
	}

	.sec-days .item-night {
		margin-top: 20rpx;
	}

	.sec-days .item-daytime-icon,
	.sec-days .item-night-icon {
		opacity: .5;
		width: 48rpx;
		height: 48rpx;
		display: block;
		margin: 0 auto;
	}

	.sec-days .item-wind {
		font-size: 24rpx;
		color: #b2b2b2;
		line-height: 24rpx;
		height: 24rpx;
		width: 80rpx;
		margin: 30rpx auto 18rpx;
		overflow: hidden;
	}

	.sec-days .item-night-weather {
		margin: 32rpx 0 40rpx;
		display: block;
	}

	.sec-days .item-temperature {
		font-size: 26rpx;
		margin-top: 30rpx;
		font-family: font-price;
		position: relative;
	}

	.sec-days .item-temperature-min {
		color: #00B3FF;
	}

	.sec-days .item-temperature-max {
		color: #FF9900;
	}

	.sec-days .item-temperature-pie {
		color: #888;
		margin: 0 4rpx;
	}

	.sec-days .item-temperature-max:after {
		content: "\B0";
		position: absolute;
		text-align: left;
		top: 0;
	}



	/* 预报 */
	.sec-hours {
		height: 270rpx;
		border-bottom: 2rpx solid #e4e4e4;
		background-color: #fff;
		overflow-x: auto;
		overflow-y: hidden;
		-webkit-overflow-scrolling: touch;
		margin-bottom: 20rpx;
		padding-top: 60rpx;
		display: block;
		max-width: 1000rpx;
		margin: 20rpx auto 0 auto;
		box-sizing: border-box;
	}

	.sec-hours-scroll {
		width: 400%;
		overflow: hidden;
	}

	.sec-hours-weather {
		width: 100%;
	}

	.sec-hours .item {
		float: left;
		width: calc(100% / 25);
		text-align: center;
		position: relative;
	}

	.sec-hours .item-time {
		font-size: 24rpx;
		line-height: 24rpx;
		height: 24rpx;
		color: #999;
	}

	.sec-hours .item-icon {
		height: 48rpx;
		width: 48rpx;
		display: block;
		margin: 30rpx auto;
	}

	.sec-hours .item-degree {
		font-size: 28rpx;
		color: #434343;
		line-height: 28rpx;
		height: 28rpx;
		position: relative;
		font-family: font-price;
	}

	.sec-hours .item-degree:after {
		content: "\B0";
		position: absolute;
		text-align: left;
		top: 0;
	}

	/* 预报 */
	.sec-tomorrow {
		background: #fff;
		height: 206rpx;
		padding-top: 52rpx;
		border-bottom: 2rpx solid #e4e4e4;
		margin-bottom: 20rpx;
		position: relative;
		display: block;
		max-width: 1000rpx;
		margin: 0 auto;
		box-sizing: border-box;
	}

	.sec-tomorrow {
		box-sizing: border-box;
	}

	.sec-tomorrow .item {
		float: left;
		width: 50%;
		height: 104rpx;
		font-size: 28rpx;
		color: #434343;
		line-height: 28rpx;
		padding: 0 40rpx;
		box-sizing: border-box;
	}

	.sec-tomorrow .item-top {
		height: 24rpx;
		margin-bottom: 26rpx;
	}

	.sec-tomorrow .item-top-date {
		float: left;
		color: #888;
	}

	.sec-tomorrow .item-top-temperature {
		font-family: font-price;
		float: right;
	}

	.sec-tomorrow .item-bottom {
		height: 44rpx;
	}

	.sec-tomorrow .item-bottom-weather {
		float: left;
		height: 44rpx;
		line-height: 44rpx;
		font-size: 32rpx;
	}

	.sec-tomorrow .item-bottom-logo {
		float: right;
		height: 44rpx;
		width: 44rpx;
	}

	.sec-tomorrow :after {
		content: "";
		position: absolute;
		width: 2rpx;
		height: 104rpx;
		background-color: #f0f0f0;
		left: 50%;
		top: 52rpx;
	}



	/* 主要 */
	.sec-main {
		height: 900rpx;
